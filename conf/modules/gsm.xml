<!DOCTYPE module SYSTEM "module.dtd">

<module name="gsm">
  <header>
    <file name="gsm.h"/>
  </header>
  <init fun="gsm_init()"/>
  <periodic fun="gsm_periodic_1Hz()" period="1." start="gsm_start()" stop="gsm_stop()"/>
  <periodic fun="gsm_send_report()" period="60."/>
  <event fun="gsm_event()"/>
  <makefile>
    <file name="gsm.c"/>
  </makefile>
</module>

